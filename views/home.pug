extends base
mixin TeamList(team)
    .main-ourteam__imageBox
        img.main-ourteam__imageBox--image(src=`img/team/${team.photo}` alt='#')
        .main-ourteam__imageBox--imageBody
            h3.main-ourteam__imageBox--heading= team.name
            p.main-ourteam__imageBox--para= team.profession
block cssStyle
    link(rel='stylesheet' href='/css/style.css')
block contents
    header.header
        nav.header-nav
            .header-nav__logo
                a.header-nav__logo-body(href='#') ExampleTech
            .burgerMenu
                img(src='/img/burgerMenu.jpg' alt='burger')
            .header-nav__menu.close
                .header-nav__menu--cross &cross;
                ul.header-nav__menu--lists
                    li.header-nav__menu--list
                        a(href='/') Home
                    li.header-nav__menu--list
                        a(href='#sectionI') About
                    li.header-nav__menu--list
                        a(href='#sectionII') Team
                    li.header-nav__menu--list
                        a(href='#sectionIII') Services
                    li.header-nav__menu--list
                        a(href='/career') Career
        .header-body
            .header-body__title
                h1 We are the best tech company in Nepal
            button.header-body__cta.btn Get in Touch
        .header-body__downarrow
            | &#x2304;

    main
        section#sectionI.main-aboutus
            h1.main-aboutus__heading.main-heading Who We Are
            .main-aboutus__body

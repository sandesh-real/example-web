        section.main-contactus
            h1.main-contactus__heading.main-heading Contact Us
            .main-contactus__container
                .main-contactus__companyinfo
                    .main-contactus__upperPart
                        .main-contactus__contactInfo
                            img.main-contactus__contactInfo--img(src='img/location.jpg' alt='location')
                            .main-contactus__locationContainer
                                h2 Location
                                p Butwal tira
                        .main-contactus__contactInfo
                            img.main-contactus__contactInfo--img(src='img/emai.jpg' alt='email')
                            .main-contactus__locationContainer
                                h2 Email
                                p email@Exampletech.com
                        .main-contactus__contactInfo
                            img.main-contactus__contactInfo--img(src='img/phone.jpg' alt='phone')
                            .main-contactus__locationContainer
                                h2 Phone
                                p +9779876543210
                    .main-contactus__map
                        iframe.main-contactus__map--mapbody(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1802079.6860416366!2d81.42240698935353!3d28.09509228452946!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3996792b4a4e0281%3A0xc835dc3d3bbe52ea!2sLumbini%20Province!5e0!3m2!1sen!2snp!4v1646659764604!5m2!1sen!2snp' width='600' height='450' style='border:0;' allowfullscreen='' loading='lazy')
                .main-contactus__contactform
                    form.main-contactus__form
                        .main-contactus__inputContainer
                            label.main-contactus__inputLabel First Name
                            input(type='text' id="firstname" name='firstname' required='')
                        .main-contactus__inputContainer
                            label.main-contactus__inputLabel Last Name
                            input(type='text' id="lastname" name='lastname' required='')
                        .main-contactus__inputContainer
                            label.main-contactus__inputLabel Email
                            input(type='email' id="email" name='email' required='')
                        .main-contactus__inputContainer
                            label.main-contactus__inputLabel Subect
                            input(type='text' id="subject" name='subject' required='')
                        .main-contactus__inputContainer
                            label.main-contactus__inputLabel Message
                            textarea(name='message' id="message" rows='10' required='')
                        .main-contactus__inputContainer
                            button.main-contactus__btn.btn Send Message
